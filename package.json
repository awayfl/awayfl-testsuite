{
	"name": "awayfl-testsuite",
	"version": "0.0.2",
	"description": "Testsuite for AwayFL",
	"scripts": {
		"extractClassesFromSWC": "node ./transpiler/ExtractClassesFromSWC.js",
		"transpile": "node --inspect ./transpiler/Transpile.js",
		"rimraf": "rimraf",
		"prettier": "prettier --write src_ts/**/*.ts",
		"rollup": "rollup -c",
		"webpack": "webpack",
		"clean": "npm cache clean && npm run rimraf -- node_modules bin",
		"clean:bin": "npm run rimraf -- bin",
		"clean:libs": "npm run rimraf -- libs",
		"preclean:install": "npm run clean",
		"clean:install": "npm set progress=false && npm install",
		"preclean:start": "npm run clean",
		"clean:start": "npm start",
		"watch": "npm run watch:dev",
		"watch:dev": "npm run build:dev -- --watch",
		"watch:dev:hmr": "npm run watch:dev -- --hot",
		"watch:test": "npm run test -- --auto-watch --no-single-run",
		"watch:prod": "npm run build:prod -- --watch",
		"build": "npm run build:dev",
		"prebuild:dev": "npm run clean:bin",
		"build:dev": "webpack --config webpack.config.js --progress",
		"build:testsuite": "webpack --config webpack.testsuite.config.js --progress",
		"prebuild:prod": "npm run clean:bin",
		"build:prod": "webpack --config webpack.config.js  --progress --env.prod",
		"server": "npm run server:dev",
		"server:dev": "webpack-dev-server --config webpack.config.js --progress --watch --hot",
		"server:testsuite": "webpack-dev-server --config webpack.testsuite.config.js --progress --watch --hot",
		"server:dev:hmr": "npm run server:dev -- --hot",
		"server:prod": "http-server bin --cors",
		"start": "npm run server:dev",
		"start:testsuite": "npm run server:testsuite",
		"start:hmr": "npm run server:dev:hmr",
		"yarnImport": "npm run rimraf -- yarn.lock && yarn import && git add ./yarn.lock && git commit -m \"update yarn.lock file\" || exit 0",
		"copyVersionToIndex": "node ./scripts/copyVersionToIndex.js /src/TestPlayer.ts && node ./scripts/copyVersionToIndex.js /src/TestSuite.tsx && git add ./src/TestSuite.tsx && git commit -m \"update version number in Main.ts\"",
		"addVersion": "git add ./package.json && cross-var git commit -m \"$npm_package_name v$npm_package_version\"",
		"version": "npm run yarnImport && npm run copyVersionToIndex && npm run build:prod",
		"postversion": "npm run addVersion && git push",
		"testServer": "node ./TestServer.js",
		"test": "jest"
	},
	"keywords": [
		"AwayJS",
		"3D",
		"2D",
		"graphics",
		"WebGL"
	],
	"author": "Rob Bateman",
	"license": "Apache-2.0",
	"dependencies": {
		"@awayfl/avm1": "^0.2.0",
		"@awayfl/avm2": "^0.2.0",
		"@awayfl/awayfl-player": "^0.2.0",
		"@awayfl/playerglobal": "^0.2.0",
		"@awayfl/swf-loader": "^0.4.0",
		"@awayjs/core": "^0.9.0",
		"@awayjs/graphics": "^0.5.0",
		"@awayjs/materials": "^0.6.0",
		"@awayjs/renderer": "^0.11.0",
		"@awayjs/scene": "^0.13.0",
		"@awayjs/stage": "^0.11.0",
		"@awayjs/view": "^0.6.0",
		"@rauschma/stringio": "^1.4.0",
		"@types/react": "^16.9.36",
		"@types/ws": "^7.2.6",
		"blueimp-canvas-to-blob": "^3.27.0",
		"child_process": "^1.0.2",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"formidable": "^1.2.2",
		"http": "^0.0.1-security",
		"immer": "^9.0.6",
		"isomorphic-ws": "^4.0.1",
		"jest": "^26.0.1",
		"jest-puppeteer": "^4.4.0",
		"pixelmatch": "^5.2.1",
		"pngjs": "^5.0.0",
		"puppeteer": "^4.0.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-icons-kit": "^1.3.1",
		"react-redux": "^7.2.0",
		"react-select": "^3.1.0",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"redux-thunks": "^1.0.0",
		"source-map-loader": "^1.0.0",
		"tslib": "^1.9.0",
		"url": "^0.11.0",
		"ws": "^7.3.0"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"copy-webpack-plugin": "^5.1.1",
		"cross-var": "^1.1.0",
		"fs": "^0.0.1-security",
		"html-webpack-plugin": "^4.2.0",
		"path": "^0.12.7",
		"rimraf": "^3.0.2",
		"rollup": "^1.10.1",
		"rollup-plugin-commonjs": "^9.3.4",
		"rollup-plugin-gzip": "^2.4.0",
		"rollup-plugin-node-resolve": "^4.2.3",
		"rollup-plugin-terser": "^5.0.0",
		"rollup-plugin-typescript2": "^0.21.0",
		"ts-loader": "^7.0.1",
		"typescript": "^3.8.3",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-merge": "^4.2.2"
	}
}
